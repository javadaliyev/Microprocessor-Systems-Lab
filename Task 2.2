
void setup() {
  Serial.begin(9600);

uint8_t R18;  // Declaration of unsigned integer

asm volatile (
    "ldi r16, 5\n\t" // Load 5 to r16
    "ldi r17, 8\n\t" // Load 8 to r17
    "add r16, r17\n\t" // Add the two numbers and store in r16
    "mov r18, r16\n\t"    // Put result into register r18
    "mov %0, r18\n\t"     // Copy r18 into C variable R18
    : "=r" (R18)          // Output R18
    :                     // No inputs
    : "r16", "r17", "r18" // Clobbered List
);

Serial.println(R18);  // Print 13


  Serial.println(R18);
}

void loop() {
  // loop is empty
}
